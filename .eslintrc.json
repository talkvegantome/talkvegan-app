{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "fetch": false
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "settings":{
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/display-name": 0,
      "react/prop-types": 0,
      "no-useless-escape": 0
    }
}
